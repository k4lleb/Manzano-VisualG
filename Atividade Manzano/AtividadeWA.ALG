algoritmo "QuadradosDe15a200"
var
   i, quadrado: inteiro
inicio
   // Loop para calcular e apresentar os quadrados de 15 a 200
   para i := 15 até 200 faça
      quadrado := i * i // Calcula o quadrado do número
      escreva("O quadrado de ", i, " é: ", quadrado, "\n")
   fimpara
fimalgoritmo
